    "@testing-library/user-event": "^12.6.0",
    "@tophat/eslint-config": "^0.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.18.0",
