{
  "name": "fe-nametbd",
  "version": "0.0.1",
  "description": "Front End Application For nametbd ",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode development",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --mode production",
    "flint": "eslint --ext .js src/ --fix",
    "lint": "eslint --ext .js src",
    "test": "jest --passWithNoTests --config ./jest.config.json",
    "test-watch": "yarn test --watch"
  },
  "keywords": [],
  "author": "nametbd",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn lint",
      "yarn test"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@tophat/eslint-config": "^0.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.4.0",
    "prettier": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "auth0-lock": "^11.27.0",
    "core-js": "^3.6.5",
    "logrocket": "^1.0.14",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "regenerator-runtime": "^0.13.7",
    "styled-components": "^5.2.0"
  }
